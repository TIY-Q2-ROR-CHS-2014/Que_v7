= form_for @que, html: { multipart: true } do |f|
  = render 'layouts/error_messages', object: @que
  = f.label :name
  = f.text_field :name, placeholder: t('activerecord.attributes.que.name')
  %br
  = f.label :email
  = f.text_field :email, placeholder: t('activerecord.attributes.que.email')
  %br
  = f.file_field :image
  %br
    = f.submit
= link_to "Home", root_path